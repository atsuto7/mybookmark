<div class="form-header">
</div>
<div class="form-box">
 <div class="url-form">
  <div class="title">
   <p>URL登録</p>
  </div>
  <%= form_for @url do |f| %>
          <div style="margin: 8px 0">
            <%= f.text_field :url, placeholder: 'url', value: '' %>
          </div>

          <div style="margin: 8px 0" class="tag-field">
           <div class="defined-result-border">
             <p class="field-title">#タグ</p>
            <div class="defined-result">
            </div>
           </div>
            <%= f.text_field :title, placeholder: '追加したいタグを入力', class: 'post-research', style: 'width: 40%;height: 40px;', autocomplete: "off" %>
            <p class="create-new-tag">新規作成</p>
            <div class="post-research__tag-search-result">
            </div>
          </div>

           <div style="margin: 8px 0" class="memo-field">
            <%= f.text_area :memo, placeholder: 'メモ', class: 'memo-box', style: 'width: 100%;height: 200px;' %>
          </div>
        
          <div class="input-btn">
            <%= f.submit "登録" %>
          </div>
          <% end %>
 </div>
</div>
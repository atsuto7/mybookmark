<div class="card">
   <div class="card-image waves-effect waves-block waves-light">
      <img class="activator" src="/screenshots/<%= url.image %>.png">
   </div>
   <div class="card-content">
      <span class="card-title activator grey-text text-darken-4"><%= url.title %><i class="material-icons right">MEMO</i></span>
      <p class="link-text"><a href="<%= url.url %>">このページを見る</a></p>
      <%= link_to '削除', "/urls/#{url.id}", class: "destroy", method: :delete %>
   </div>
   <div class="card-reveal">
      <span class="card-title grey-text text-darken-4"><%= url.title %><i class="material-icons right">close</i></span>
      <div class="card-memo-content">
      <p class="memo-title">MEMO: </p><p><%= url.memo %></p>
      </div>
   </div>
  </div>